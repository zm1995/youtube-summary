{
    "title": "YouTube Video Summary Scraper",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "start_urls": {
            "title": "Start URLs",
            "type": "array",
            "description": "YouTube URLs to start scraping from (homepage, search, channel, etc.)",
            "prefill": [{ "url": "https://www.youtube.com" }],
            "default": [{ "url": "https://www.youtube.com" }],
            "editor": "requestListSources"
        },
        "max_videos": {
            "title": "Max Videos",
            "type": "integer",
            "description": "Maximum number of videos to scrape (default: 30)",
            "default": 30,
            "minimum": 1,
            "maximum": 1000
        }
    },
    "required": ["start_urls"]
}
